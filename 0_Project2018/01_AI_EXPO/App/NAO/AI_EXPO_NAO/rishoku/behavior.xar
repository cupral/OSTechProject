            rate = self.getParameter("Confidence threshold (%)") / 100.0
            self.dialog.setASRConfidenceThreshold(rate)
            self.logger.info("Dialog Confidence Threshold changed from :%0.2f to %0.2f" % (self.prevThreshold, rate))
            self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
