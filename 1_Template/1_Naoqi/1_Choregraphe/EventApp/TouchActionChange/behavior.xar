                                                                                        <script language="4">
                                                                                            <content>
                                                                                                <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.animated = ALProxy("ALAnimatedSpeech")
        self.memory = ALProxy("ALMemory")
        self.tts = ALProxy("ALTextToSpeech")

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        self.sign = False
        self.tts.stopAll()
        #put clean-up code here
        pass

    def onInput_onStart(self):
        self.sign = True
        index = 1
        while self.sign == True:
            if index == 1:
                self.animated.say(str( self.getParameter("say1") ))
                self.memory.raiseEvent("NaoEvent/Image01",1)
                index += 1
            elif index == 2:
                self.animated.say(str( self.getParameter("say2") ))
                self.memory.raiseEvent("NaoEvent/Image02",1)
                index += 1
            elif index == 3:
                self.animated.say(str( self.getParameter("say3") ))
                self.memory.raiseEvent("NaoEvent/Image03",1)
                index += 1
            elif index == 4:
                self.animated.say(str( self.getParameter("say4") ))
                self.onStopped()
                self.sign = False
                break
            else:
                break
        else:
            self.logger.info("WhileError")




    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onFinish() #activate the output of the box

    def onInput_onWait(self):
        self.onUnload()
        self.onWaited()]]>
